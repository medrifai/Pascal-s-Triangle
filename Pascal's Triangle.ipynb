   "source": [
    "def generate(numRows):\n",
    "    L = []\n",
    "    \n",
    "    for i in range(numRows):\n",
    "        L.append([1] * (i + 1))\n",
    "\n",
    "    for j in range(2, numRows):\n",
    "        for i in range(1, len(L[j]) - 1):\n",
    "            L[j][i] = L[j-1][i-1] + L[j-1][i]\n",
    "    \n",
    "    return L"
   ]
